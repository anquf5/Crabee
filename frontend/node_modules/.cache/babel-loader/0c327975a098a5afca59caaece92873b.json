OAFN;AAUJ,MAAA,cAAc,EAAC,EAVX;AAWJ,MAAA,QAAO,EAAI;AAXP,KAAN;AAaD,GAjBY;AAkBb,EAAA,UAAU,EAAE;AACV,IAAA,cAAc,EAAd,cADU;AAEV,IAAA,UAAU,EAAV,UAFU;AAGV,IAAA,IAAG,EAAH;AAHU,GAlBC;AAuBb,EAAA,OAvBa,qBAuBH;AACR,SAAK,UAAL;AACA,SAAK,iBAAL;AACD,GA1BY;AA2Bb,EAAA,OAAO,EAAE;AACP,IAAA,UADO,wBACK;AAAA;;AACV,UAAM,EAAE,GAAC,KAAK,MAAL,CAAY,MAAZ,CAAmB,EAA5B;AAEA,MAAA,KAAI,CACD,GADH,wBACuB,EADvB,QAEG,IAFH,CAEQ,UAAA,QAAO,EAAK;AAChB,QAAA,KAAI,CAAC,OAAL,GAAe,QAAQ,CAAC,IAAxB;AACD,OAJH,EAKG,KALH,CAKS,UAAA,KAAI,EAAK;AACd,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OAPH;AAQD,KAZM;AAaP,IAAA,iBAbO,+BAaY;AAAA;;AACjB,MAAA,KAAI,CACC,GADL,CACS,eADT,EAEK,IAFL,CAEU,UAAA,QAAO,EAAK;AAChB,QAAA,MAAI,CAAC,cAAL,GAAsB,QAAQ,CAAC,IAA/B,CADgB,CAEZ;AACL,OALL,EAMK,KANL,CAMW,UAAA,KAAI,EAAK;AACd,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OARL;AASD,KAvBM;AAwBP,IAAA,SAxBO,uBAwBI;AAAA;;AACT,MAAA,KAAK,CAAC,IAAN,CAAW,0BAAX,EAAuC;AACrC,eAAO,KAAK,QAAL,CAAc,EADgB;AAErC,eAAO,KAAK,QAAL,CAAc,SAFgB;AAGrC,iBAAS,KAAK,QAAL,CAAc,KAHc;AAIrC,mBAAW,KAAK,QAAL,CAAc,OAJY;AAKrC,gBAAQ,KAAK,QAAL,CAAc,IALe;AAMrC,sBAAc,KAAK,QAAL,CAAc;AANS,OAAvC,EAQC,IARD,CAQM,UAAA,QAAO,EAAK;AAChB,QAAA,MAAI,CAAC,SAAL;;AACA,QAAA,QAAQ,CAAC,MAAT;AACD,OAXD;AAaA,MAAA,KAAK,CAAC;AACJ,QAAA,OAAO,EAAE,wBADL;AAEJ,QAAA,IAAI,EAAE,YAFF;AAGJ,QAAA,WAAW,EAAE,IAHT;AAIJ,QAAA,YAAY,EAAE,IAJV;AAKJ,QAAA,QAAQ,EAAE,IALN;AAMJ,QAAA,QAAQ,EAAE;AANN,OAAD,CAAL;AAQD,KA9CM;AA+CP,IAAA,QA/CO,sBA+CG;AACR,WAAK,QAAL,GAAgB,IAAhB;AACD,KAjDM;AAkDP,IAAA,SAlDO,uBAkDI;AACT,WAAK,QAAL,GAAgB,KAAhB;AACD,KApDM;AAqDP,IAAA,IArDO,gBAqDF,IArDE,EAqDG;AACR,MAAA,MAAM,CAAC,QAAP,CAAgB,IAAhB,GAAqB,IAArB;AACF;AAvDO;AA3BI,CAAf","sourcesContent":["<template>\r\n  <div class=\"page-company\">\r\n    <section class=\"section\">\r\n      <div class=\"columns\">\r\n        <div class=\"column is-8\">\r\n          <div id=\"banner\">\r\n            <section class=\"hero\">\r\n              <div class=\"hero-body has-background-light\">\r\n                <article class=\"media\">\r\n                  <div class=\"media-left\">\r\n                    <figure class=\"image is-128x128\">\r\n                      <img v-bind:src=\"company.get_image\" alt=\"Image\">\r\n                    </figure>\r\n                  </div>\r\n                  <div class=\"media-content\">\r\n                    <div class=\"content ml-4\">\r\n                      <span class=\"icon-text\">\r\n                        <span class=\"mr-2\"><h2><strong>{{ company.name }}</strong></h2></span>\r\n                        <span class=\"icon has-text-info-dark\">\r\n                          <a @click=\"jump(company.link)\" style=\"cursor:pointer;\">\r\n\r\n                          </a>\r\n                          <i class=\"fas fa-external-link-alt is-medium fa-lg\" @click=\"jump(company.link)\"></i>\r\n                        </span>\r\n                      </span>\r\n                      <div class=\"ml-1\">\r\n                        <star :size=\"36\" :score=\"company.get_avg\"/>\r\n                      </div>\r\n                      <p class=\"mt-4 ml-1\">{{ company.intro }} </p>\r\n                    </div>\r\n                  </div>\r\n                </article>\r\n              </div>\r\n            </section>\r\n          </div>\r\n          <div id=\"review-title\" class=\"mt-6\">\r\n            <div class=\"content\">\r\n              <h3><strong>Reviews <small>({{ company.get_review_num }})</small></strong></h3>\r\n            </div>\r\n            <button class=\"button is-success modal-button\" data-target=\"review-form\" @click=\"showForm\">\r\n                  <span class=\"icon-text\">\r\n                    <span class=\"icon\">\r\n                      <i class=\"fas fa-plus\"></i>\r\n                    </span>\r\n                    <span><strong>Write a review</strong></span>\r\n                  </span>\r\n            </button>\r\n            <div class=\"modal\" v-bind:class=\"{'is-active': isActive}\">\r\n              <div class=\"modal-background\"></div>\r\n              <div class=\"modal-card\">\r\n                <header class=\"modal-card-head\">\r\n                  <p class=\"modal-card-title\">Add Review</p>\r\n                  <button class=\"delete\" aria-label=\"close\" @click=\"closeForm\"></button>\r\n                </header>\r\n                <section class=\"modal-card-body\">\r\n                  <form @submit.prevent=\"addReview\">\r\n\r\n                    <div class=\"field\">\r\n                      <label class=\"label\">Rate</label>\r\n                      <div class=\"control\">\r\n                        <div class=\"select\">\r\n                          <select v-model=\"formData.rate\">\r\n                            <option>1</option>\r\n                            <option>2</option>\r\n                            <option>3</option>\r\n                            <option>4</option>\r\n                            <option>5</option>\r\n                          </select>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n\r\n                    <div class=\"field\">\r\n                      <label class=\"label\">Interview difficulty</label>\r\n                      <div class=\"control\">\r\n                        <div class=\"select\">\r\n                          <select v-model=\"formData.difficulty\">\r\n                            <option>Easy</option>\r\n                            <option>Normal</option>\r\n                            <option>Difficult</option>\r\n                          </select>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n\r\n                    <div class=\"field\">\r\n                      <label class=\"label\">Job</label>\r\n                      <div class=\"control\">\r\n                        <input class=\"input\" placeholder=\"Job Title\" v-model=\"formData.job_title\">\r\n                      </div>\r\n                    </div>\r\n\r\n                    <div class=\"field\">\r\n                      <label class=\"label\">Review title</label>\r\n                      <div class=\"control is-medium\">\r\n                        <input class=\"input\" placeholder=\"Title\" v-model=\"formData.title\">\r\n                      </div>\r\n                    </div>\r\n\r\n                    <div class=\"field\">\r\n                      <label class=\"label\">Content</label>\r\n                      <div class=\"control\">\r\n                        <textarea class=\"textarea\" type=\"text\" placeholder=\"Write your review...\" v-model=\"formData.content\"></textarea>\r\n                      </div>\r\n                    </div>\r\n\r\n                    <div class=\"field\">\r\n                      <p class=\"control\">\r\n                        <button class=\"button is-success\"><strong>Post review</strong></button>\r\n                      </p>\r\n                    </div>\r\n                  </form>\r\n                </section>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div id=\"reviews\" class=\"mt-6\">\r\n            <div v-for=\"review in company.review\" v-bind:key=\"review.id\" v-bind:company=\"company\">\r\n              <div class=\"columns\">\r\n                <div class=\"column is-2 mt-3\">\r\n                  <div class=\"image is-64x64\">\r\n                    <div v-if=\"review.get_user_avatar !== '' \">\r\n                      <img v-bind:src=\"review.get_user_avatar\">\r\n                    </div>\r\n                    <div v-else>\r\n                      <img :src=\"require('../assets/default.jpg')\">\r\n                    </div>\r\n                  </div>\r\n                  <p>{{ review.get_reviewer }}</p>\r\n                </div>\r\n                <div class=\"column is-7\">\r\n\r\n                  <div>\r\n                    <p class=\"is-size-5 has-text-grey-darker\">\r\n                      <strong>{{ review.review_title }}</strong>\r\n                    </p>\r\n                    <p class=\"is-size-6 has-text-grey ml-4 mt-4\">{{ review.job_title }}</p>\r\n                  </div>\r\n                  <div class=\"block\">\r\n                    <div class=\"columns is-multiline mt-1\">\r\n                      <div class=\"column is-6\">\r\n                        <p class=\"is-size-6 has-text-grey ml-4\">Rate: <star :size=\"36\" :score=\"review.rate\"/></p>\r\n                      </div>\r\n                      <div class=\"column is-1\">\r\n                        <p class=\"is-size-6 has-text-grey\">Difficulty:</p>\r\n                      </div>\r\n                      <div class=\"column is-4\">\r\n                        <star :size=\"36\" :score=\"review.iv_difficulty\"/>\r\n                      </div>\r\n                    </div>\r\n                    <p class=\"is-size-6 has-text-grey-darker\">{{ review.review_cont }}</p>\r\n                  </div>\r\n                </div>\r\n                <div class=\"column is-3\">\r\n                  <p class=\"is-size-6 has-text-grey\">{{ review.get_pubtime }}</p>\r\n                </div>\r\n              </div>\r\n              <hr>\r\n            </div>\r\n          </div>\r\n\r\n        </div>\r\n        <div class=\"column is-4\">\r\n          <OtherCompanies></OtherCompanies>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport { toast } from 'bulma-toast'\r\nimport CompanyBox from \"../components/CompanyBox.vue\";\r\nimport OtherCompanies from \"../components/OtherCompanies\";\r\nimport star from \"../components/star\";\r\nexport default {\r\n  name: \"Company\",\r\n  data(){\r\n\r\n    return{\r\n      company:{},\r\n      formData: {\r\n        id: this.$route.params.id,\r\n        rate: '',\r\n        difficulty: '',\r\n        job_title: '',\r\n        title: '',\r\n        content:'',\r\n      },\r\n      otherCompanies:[],\r\n      isActive : false\r\n    }\r\n  },\r\n  components: {\r\n    OtherCompanies,\r\n    CompanyBox,\r\n    star\r\n  },\r\n  mounted() {\r\n    this.getCompany()\r\n    this.getOtherCompanies()\r\n  },\r\n  methods: {\r\n    getCompany(){\r\n      const id=this.$route.params.id\r\n\r\n      axios\r\n        .get(`/api/company/${id}/`)\r\n        .then(response => {\r\n          this.company = response.data\r\n        })\r\n        .catch(error => {\r\n          console.log(error)\r\n        })\r\n    },\r\n    getOtherCompanies(){\r\n      axios\r\n          .get('/api/company/')\r\n          .then(response => {\r\n            this.otherCompanies = response.data\r\n                // .filter(company => company.id !== this.cid)\r\n          })\r\n          .catch(error => {\r\n            console.log(error)\r\n          })\r\n    },\r\n    addReview(){\r\n      axios.post('/api/company_review/add/', {\r\n        'cid': this.formData.id,\r\n        'job': this.formData.job_title,\r\n        'title': this.formData.title,\r\n        'content': this.formData.content,\r\n        'rate': this.formData.rate,\r\n        'difficulty': this.formData.difficulty,\r\n      })\r\n      .then(response => {\r\n        this.closeForm();\r\n        location.reload();\r\n      })\r\n\r\n      toast({\r\n        message: 'This review was added!',\r\n        type: 'is-success',\r\n        dismissible: true,\r\n        pauseOnHover: true,\r\n        duration: 2000,\r\n        position: 'bottom-right',\r\n      })\r\n    },\r\n    showForm(){\r\n      this.isActive = true\r\n    },\r\n    closeForm(){\r\n      this.isActive = false\r\n    },\r\n    jump(link){\r\n      window.location.href=link;\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"sourceRoot":""}]}